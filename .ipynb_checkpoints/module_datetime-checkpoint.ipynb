    "import datetime\n",
    "a = input().split()\n",
    "y, m, d = int(a[0]), int(a[1]), int(a[2])\n",
    "days = int(input())\n",
    "d1 = datetime.date(y, m, d)\n",
    "delta = datetime.timedelta(days)\n",
    "result = d1 + delta\n",
    "print(result.year, result.month, result.day)"
   ]
